    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.4",
    "@types/jest": "^25.1.3",
    "@types/jest-when": "^2.7.0",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/node": "^13.9.8",
    "dockest": "^2.0.1",
    "jest": "^25.2.4",
    "jest-extended": "^0.11.5",
    "jest-when": "^2.7.1",
    "nodemon": "^2.0.2",
    "pg": "^7.18.2",
    "rimraf": "^3.0.2",
